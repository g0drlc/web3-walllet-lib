{
  "name": "@web3-wallet/plugin-connection-status-react",
  "description": "web3-wallet connection status plugin for tracking connections status",
  "version": "0.9.7",
  "author": "logan <logan.luo@crypto.com>",
  "license": "MIT",
  "keywords": [
    "web3-wallet",
    "react"
  ],
  "files": [
    "**"
  ],
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "tsc;tsc --module commonjs --outDir dist/cjs",
    "watch": "chokidar 'src/**/*.ts' -c 'pnpm -wc exec turbo run build --filter=plugin-connection-status-react'",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "ts-check": "tsc --noEmit"
  },
  "dependencies": {
    "@web3-wallet/react": "workspace:*",
    "zustand": "^4.0.0-rc.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "vue": ">=3.2.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-wallet/web3-wallet.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/web3-wallet/web3-wallet/issues"
  },
  "homepage": "https://github.com/web3-wallet/web3-wallet#readme"
}
